{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "spread" : true,
            "restParams" : true
        }
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules":{
        "linebreak-style": 0
    }
}